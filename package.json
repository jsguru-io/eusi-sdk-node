{
  "name": "eusi-sdk-node",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsguru-io/eusi-sdk-node"
  },
  "description": "SDK for accessing EUSI delivery API using NodeJS",
  "main": "dist/eusi-sdk-node.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sample": "nodemon samples/sample.js --exec babel-node --presets env",
    "build": "rollup --config",
    "gen-content-table": "doctoc README.md",
    "precommit": "lint-staged && npm run build && git add --all"
  },
  "keywords": [
    "eusi",
    "node",
    "sdk",
    "EUSI",
    "delivery",
    "API"
  ],
  "author": "JS Guru International",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-preset-env": "^1.6.1",
    "core-js": "^2.5.3",
    "doctoc": "^1.3.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "nodemon": "^1.14.12",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-pluginutils": "^2.0.1"
  },
  "dependencies": {
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "peerDependencies": {
    "eusi-sdk-core": "^1.0.0"
  },
  "files": [
    "dist",
    "lib"
  ],
  "lint-staged": {
    "lib/**/*.js": [
      "eslint --fix"
    ]
  }
}
